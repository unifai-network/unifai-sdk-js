# required if you want to use tools
UNIFAI_AGENT_API_KEY=

# required if you want to create toolkits
UNIFAI_TOOLKIT_API_KEY=

# Anthropic API Key (required for use tool examples)
ANTHROPIC_API_KEY=

# EVM Private Key (optional, for EVM transaction signing)
# Format: 0x... (hex string with 0x prefix)
EVM_PRIVATE_KEY=

# Solana Private Key (optional, for Solana transaction signing)
# Supports multiple formats:
# - Base58 string (most common, from Phantom/Solflare): 5Kd7...
# - JSON array: [1,2,3,...]
# - Hex string: abc123...
SOLANA_PRIVATE_KEY=
